import java.io.*;
import java.util.*;
import java.math.*;
public class Solution {
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in) ;
        int a=sc.nextInt();
        if(a==0)
        {
         System.out.println("zero");  
        }
        else if((a<0)&&(a<=-1))
        {
            System.out.println("negative");
        }
        else{
            System.out.println("positive");
        }
    }
}
