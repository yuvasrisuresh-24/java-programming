import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int a;float b;
      Scanner sc=new Scanner(System.in);
        a=sc.nextInt();
         b=sc.nextFloat();
        System.out.println(+a);
        System.out.printf("%.2f",b);
        
    }       
}
