import java.io.*;
import java.util.*;
public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        long n=sc.nextLong();
        if(n>0&&n<1000000000){
            int rem=0;
            while(n>0){
            int rev=(int)n%10;
               n=n/10;
                rem=rev;
             System.out.print(rem);
            }
        }
        else{
            System.out.println("Invalid Input");
        }
        
    }
}
